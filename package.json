{
  "name": "svg-generator-mcp",
  "version": "1.0.0",
  "description": "Professional MCP Server for generating SVG illustrations using MFLUX with Vector SVG Laser LoRA and FluxxxMix Checkpoint models",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "svg-generator-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "test": "node --test --import tsx/esm src/**/*.test.ts",
    "test:coverage": "echo 'Coverage not implemented yet'",
    "lint": "echo 'Linting disabled for now'",
    "lint:fix": "echo 'Linting disabled for now'",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "smithery:deploy": "npx @smithery/cli run"
  },
  "keywords": [
    "mcp",
    "svg",
    "ai",
    "illustration",
    "mflux",
    "lora",
    "stable-diffusion"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "commander": "^12.0.0",
    "python-shell": "^5.0.0",
    "sharp": "^0.33.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@smithery/cli": "^1.2.17",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aloshy-ai/svg-generator-mcp"
  },
  "bugs": {
    "url": "https://github.com/aloshy-ai/svg-generator-mcp/issues"
  },
  "homepage": "https://github.com/aloshy-ai/svg-generator-mcp#readme"
}
